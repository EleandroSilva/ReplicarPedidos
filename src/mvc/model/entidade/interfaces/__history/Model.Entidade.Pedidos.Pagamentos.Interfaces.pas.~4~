unit Model.Entidade.Pedidos.Pagamentos.Interfaces;

interface

type
  iEntidadePedidosPagamentos<T> = interface
    ['{6EBAACE7-2E1A-4878-BB3A-A7E83C62444F}']

    {

     select
  pgto.cod_pgto      as idpagamento,
  pgto.IdPedido      as idpedido,
  pgto.ID_PED        as idped,
  pgto.COD_PED       as codigopedido,
  pgto.ITEM          as item,
  pgto.num_pgto      as numeropagamento,
  pgto.DT_VENCIMENTO as datavencimento,
  pgto.NOVA          as nova,
  pgto.BOLETO        as boleto,/*NAO*/
  pgto.BANCO         as banco,/*999*/
  pgto.VL_PARCELA    as valorparcela,
  pgto.QTDE_DIAS     as quantidadededias,
  pgto.PG_COM        as pagocom,
  pgto.VL_TOTAL      as valordopedido,
  pgto.COMISSAO      as comissao,/*NAO*/
  pgto.LIB_COM       as liberoucomissao,/*NAO*/
  pgto.OCORRENCIA    as ocorrencia,/*01*/
  pgto.LL            as ll,/*P*/
  pgto.ENVIAR        as enviar,/*SIM*/
  pgto.OBS           as obs,
  pgto.DESCONTADO    as descontado,/*NAO*/
  pgto.CARTORIO      as cartorio/*NAO*/
from cad_pgto_ped pgto

    }

  end;

implementation

end.
