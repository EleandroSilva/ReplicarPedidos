unit Model.Entidade.Prazo.Pagamento.Itens.Interfaces;

interface

type
  iPrazoPagamentoItens = interface
    ['{094F4D84-3EBD-4B5C-8166-238B5539E7A8}']
    function Id              (Value : String)  : iPrazoPagamentoItens; overload;
    function Id                                : String;               overload;
    function Item            (Value : Integer) : iPrazoPagamentoItens; overload;
    function Item                              : Integer;              overload;
    function numeroPagamento (Value : Integer) : iPrazoPagamentoItens; overload;
    function NumeroPagamento                   : Integer;              overload;
    function QuantidadedeDias(Value : Integer) : iPrazoPagamentoItens; overload;
    function QuantidadedeDias                  : Integer;              overload;
  end;

implementation

end.
